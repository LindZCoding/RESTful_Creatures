<form action="/dinosaurs" method="GET">
    <label for="nameFilter">Filter By Name</label>  
    <input type="text" name="nameFilter" id="nameFilter"> 
    <input type="Submit">
</form>

<ul>
    <% dinoData.forEach((dino, index)=> { %>
        <!-- .name and .type comes from the console.log console.log(dinosaurs)
    console.log(dinoData) from the index.js-->
        <li>
            <%= dino.name %> is a <%= dino.type %>
                    <form action="/dinosaurs/<%= index %>/?_method=DELETE " method="POST"><input type="submit" value="delete"></form>
        </li>
        <% }) %>
</ul>