<form action="/prehistoric" method="GET">
    <label for="nameFilter">Filter By Name</label>  
    <input type="text" name="nameFilter" id="nameFilter"> 
    <input type="Submit">
</form>

<ul>
    <% prehistoricData.forEach((predino, index)=> { %>
        <li>
            <h2><%= predino.type %> </h2> <img width="500" src="<%= predino.img_url %>" alt="predino image">
                    <form action="/prehistoric/<%= index %>/?_method=DELETE " method="POST"><input type="submit" value="delete"></form>
        </li>
        <% }) %>
</ul>